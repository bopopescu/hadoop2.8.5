msgid "Minutes"
msgstr "Minuten"

#: src/oozie/models.py:1386
#: src/oozie/templates/editor2/common_scheduler.inc.mako:294
msgid "Hours"
msgstr "Stunden"

#: src/oozie/models.py:1387
#: src/oozie/templates/editor2/common_scheduler.inc.mako:295
msgid "Days"
msgstr "Tage"

#: src/oozie/models.py:1388
#: src/oozie/templates/editor2/common_scheduler.inc.mako:296
msgid "Months"
msgstr "Monate"

#: src/oozie/models.py:1394 src/oozie/models.py:1667
msgid "Frequency number"
msgstr "Häufigkeitsnummer"

#: src/oozie/models.py:1395 src/oozie/models.py:1668
